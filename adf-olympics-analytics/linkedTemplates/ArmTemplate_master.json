{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-olympics-analytics"
		},
		"http_excel_linked_service_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'http_excel_linked_service'"
		},
		"linked_service_olympic_ADLSgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_service_olympic_ADLSgen2'"
		},
		"http_excel_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/"
		},
		"http_excel_linked_service_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "saurabh.shrivastava2111@gmail.com"
		},
		"json_linked_service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"linked_service_olympic_ADLSgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccountolypics.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-olympics-analytics_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"http_excel_linked_service_password": {
						"value": "[parameters('http_excel_linked_service_password')]"
					},
					"linked_service_olympic_ADLSgen2_accountKey": {
						"value": "[parameters('linked_service_olympic_ADLSgen2_accountKey')]"
					},
					"http_excel_linked_service_properties_typeProperties_url": {
						"value": "[parameters('http_excel_linked_service_properties_typeProperties_url')]"
					},
					"http_excel_linked_service_properties_typeProperties_userName": {
						"value": "[parameters('http_excel_linked_service_properties_typeProperties_userName')]"
					},
					"json_linked_service_properties_typeProperties_url": {
						"value": "[parameters('json_linked_service_properties_typeProperties_url')]"
					},
					"linked_service_olympic_ADLSgen2_properties_typeProperties_url": {
						"value": "[parameters('linked_service_olympic_ADLSgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}